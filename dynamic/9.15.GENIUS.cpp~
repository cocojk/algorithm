
#include <iostream>
#include <stdio.h>
using namespace std;
int k,n;
int m;
int L[50];
double T [50][50];
int Q[50];
double probs [50][1000000];

void solver ()
{
    int index;
    double answer;
    int maxL = L[0];

    for (int i = 0; i < n; i++)
        maxL = max(maxL,L[i]);
    

    for (int i = 0 ; i < n; i++)
        for (int j = 0; j < k; j++)
            probs[i][j] = 0;

    probs[0][L[0]] = 1;

    for (int i = L[0]; i <= k; i++)
    {
        for (int current = 0; current < n; current++)
        {
                for (int before = 0; before < n; before++)
                {
                    probs[current][i+L[current]] += probs[before][i] * T[before][current];
               //     printf("probs[%d][%d] * T[%d][%d] = %lf(%lf * %lf)\n",before,i,before,current,probs[before][i]*T[before][current],probs[before][i],T[before][current]);
                }

                // printf("[%d][%d] : %lf\n",current,i,probs[current][i+L[current]]);

        }
    }

    for (int i = 0; i < m; i++)
    {
        cin >> index;
        answer = 0;
        for (int j = k +1 ; j <= k + maxL ; j++)
            answer += probs[index][j];
        cout << answer << " ";
    }
    cout << endl;
}

int main (void)
{
    int testcase;

    cin >> testcase;

    while (testcase--)
    {
        cin >> n >> k >> m;

        for (int i = 0; i < n; i++)
            cin >> L[i];

        for (int i = 0; i < n; i++)
            for (int j = 0; j < n; j++)
                cin >> T[i][j];

        solver();
    }
}






